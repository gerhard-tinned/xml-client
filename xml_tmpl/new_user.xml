        <user_create>#USER_CREATE#</user_create>
        <user_delete>#USER_DELETE#</user_delete>
        <nameserver_write>#NAMESERVER_WRITE#</nameserver_write>
        <nameserver_create>#NAMESERVER_CREATE#</nameserver_create>
        <nameserver_delete>#NAMESERVER_DELETE#</nameserver_delete>
        <self_write>#SELF_WRITE#</self_write>
        <inherit_group_permissions>#INHERIT_GROUP_PERMISSIONS#"</inherit_group_permissions>
